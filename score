<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Document</title>
</head>
<body>

  <script>
    function chengji1(score){
        if (score >= 90) {
          return 'A';
        }else if(score >= 80){
          return 'B'
        }else if(score >= 70){
          return 'C'
        }else if(score >= 60){
          return 'D'
        }else{
          return 'E'
        }
    }

    function chengji2(score){
        switch(true){
          case score >= 90:
              return 'A';
          case score >= 80:
              return 'B';
          case score >= 70:
              return 'C';
          case score >= 60:
              return 'D'; 
          default:
              return 'E';
        }
    
    }

    function chengji3(score){
        var s = parseInt(score /10);
        switch(s){
          case 10:
          case 9:
              return 'A';
          case 8:
              return 'B';
          case 7:
              return 'C'; 
          case 6:
              return 'D'; 
          default:
              return 'E';
        }
    }

    function chengji4(score){
        return  score >=90 ?'A':
                score >=80?'B':
                score>=70?'C':
                score>=60 ?'D':E
    }

  </script>
</body>
</html>
